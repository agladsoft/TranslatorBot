{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "env": {
    "TOKEN": "@token",
    "OPENAI_API_KEY": "@openai_api_key",
    "MOCHI_API_KEY": "@mochi_api_key",
    "UNSPLASH_ACCESS_KEY": "@unsplash_access_key",
    "WEBHOOK_URL": "@webhook_url"
  }
}
